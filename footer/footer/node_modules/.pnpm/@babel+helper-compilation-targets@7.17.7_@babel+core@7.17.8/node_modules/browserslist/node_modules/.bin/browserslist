#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/.pnpm/browserslist@4.20.2/node_modules/browserslist/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/.pnpm/browserslist@4.20.2/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/.pnpm/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/node_modules:/mnt/c/Users/harish/.vscode/.vscode/node_modules:/mnt/c/Users/harish/.vscode/node_modules:/mnt/c/Users/harish/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/.pnpm/browserslist@4.20.2/node_modules/browserslist/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/.pnpm/browserslist@4.20.2/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/.pnpm/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/node_modules:/mnt/c/Users/harish/.vscode/.vscode/node_modules:/mnt/c/Users/harish/.vscode/node_modules:/mnt/c/Users/harish/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
