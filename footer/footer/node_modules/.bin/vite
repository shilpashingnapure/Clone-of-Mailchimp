else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/.pnpm/vite@2.8.6/node_modules/vite/bin/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/.pnpm/vite@2.8.6/node_modules/vite/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/.pnpm/vite@2.8.6/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/.pnpm/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/node_modules:/mnt/c/Users/harish/.vscode/.vscode/node_modules:/mnt/c/Users/harish/.vscode/node_modules:/mnt/c/Users/harish/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/vite/bin/node_modules:/mnt/c/Users/harish/.vscode/.vscode/unit3 advance js/project u3/Clone-of-Mailchimp/footer/footer/node_modules/vite/node_modules"
